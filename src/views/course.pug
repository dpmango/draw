doctype html
<!--[if IE 8]>         <html class="ie8 oldie"> <![endif]-->
<!--[if IE 9]>         <html class="ie9 oldie"> <![endif]-->
<!--[if gt IE 9]><!--> <html class=""> <!--<![endif]-->
  include shared/head.pug

  body
    include shared/header.pug
    
    .page
      .page__content
        // PROFILE HEADING::START
        .profile-heading.profile-heading--back
          .container
            a(href='#').profile-heading__back
              img(src='images/el/backIco.png' srcset='images/el/backIco@2x.png 2x')
              span назад
            .heading-mega ЖИВОПИСЬ ДОМА
            .profile-heading__progressbar
              .profile-heading__progressbar__fill(style='width: 20%')
            .profile-heading__progressbar__status Пройдено 8/24 уроков
              
        // COURSES
        .courses
          // single course - disabled
          .course.course--disabled
            .course__video
              .course__video__status просмотрен
              // VIDEO
              .video__content
                .video__player
                  i.icon.icon-circle
                    i.icon.icon-play
                  img(src='images/tmp/video_3.jpg' srcset='images/tmp/video_3@2x.jpg 2x')
                  // place iframe with ?autoplay=0 param, don't forget about 100% width and height
                  iframe(src='https://player.vimeo.com/video/72874423?autoplay=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')
            .course__name Учимся рисовать гранаты (натюрморт)
            .course__status.course__status--compleated Выполнены все домашние задания
            
          // single course - regular
          .course
            .course__video
              .course__video__status просмотрен
              // VIDEO
              .video__content
                .video__player
                  i.icon.icon-circle
                    i.icon.icon-play
                  img(src='images/tmp/video_4.jpg' srcset='images/tmp/video_4@2x.jpg 2x')
                  // place iframe with ?autoplay=0 param, don't forget about 100% width and height
                  iframe(src='https://player.vimeo.com/video/72874423?autoplay=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')
            .course__name Рисуем груши
            .course__status.course__status--pending Осталось 1 домашнее задание
              
          // single course - regular
          .course
            .course__video
              .course__video__status просмотрен
              // VIDEO
              .video__content
                .video__player
                  i.icon.icon-circle
                    i.icon.icon-play
                  img(src='images/tmp/video_2.jpg' srcset='images/tmp/video_2@2x.jpg 2x')
                  // place iframe with ?autoplay=0 param, don't forget about 100% width and height
                  iframe(src='https://player.vimeo.com/video/72874423?autoplay=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')
            .course__name Натюрморт как он есть, чтобы посмотреть как быть с длинным названием
            .course__status Ждет выполнения
            
          // single course - disabled
          .course.course--disabled
            .course__video
              .course__video__status просмотрен
              // VIDEO
              .video__content
                .video__player
                  i.icon.icon-circle
                    i.icon.icon-play
                  img(src='images/tmp/video_3.jpg' srcset='images/tmp/video_3@2x.jpg 2x')
                  // place iframe with ?autoplay=0 param, don't forget about 100% width and height
                  iframe(src='https://player.vimeo.com/video/72874423?autoplay=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')
            .course__name Учимся рисовать гранаты (натюрморт)
            .course__status.course__status--disabled Ждет выполнения предидущего урока
        
        // BONUS COURSE
        .courses.courses--bonus
          .courses__bonus-head
            img(src='images/el/diamond.png' srcset='images/el/diamond@2x.png 2x')
            
          // single course - regular
          .course.course--bonus
            .course__video
              // VIDEO
              .video__content
                .video__player
                  i.icon.icon-circle
                    i.icon.icon-play
                  img(src='images/tmp/video_2.jpg' srcset='images/tmp/video_2@2x.jpg 2x')
                  // place iframe with ?autoplay=0 param, don't forget about 100% width and height
                  iframe(src='https://player.vimeo.com/video/72874423?autoplay=0', width='100%', height='100%', frameborder='0', webkitallowfullscreen='', mozallowfullscreen='', allowfullscreen='')
            .course__name Бонус-Урок
            .course__status Будет доступен после выполнения всех практик
          

      include shared/footer.pug
      
    include shared/scripts.pug
